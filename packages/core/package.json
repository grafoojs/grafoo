{
  "name": "@grafoo/core",
  "version": "1.4.5-next.0",
  "description": "grafoo client core",
  "repository": "https://github.com/grafoojs/grafoo/tree/master/packages/core",
  "source": "./src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "author": "malbernaz<albernazmiguel@gmail.com>",
  "license": "MIT",
  "keywords": [
    "babel",
    "babel-plugin",
    "graphql",
    "graphql-client",
    "grafoo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "microbundle",
    "watch": "microbundle watch",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx|js)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(lowdb|steno|node-fetch|fetch-blob|data-uri-to-buffer|formdata-polyfill)/)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/__tests__/data",
      "<rootDir>/__tests__/queries.ts"
    ]
  },
  "gitHead": "0bc67d8b398884a1f387a1813e485d2c5318b974"
}
