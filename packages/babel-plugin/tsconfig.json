{
  "compilerOptions": {
    "moduleResolution": "node",
    "strict": false,
    "lib": ["esnext", "dom"],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "checkJs": false,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "module": "commonjs"
  },
  "include": ["src"]
}
